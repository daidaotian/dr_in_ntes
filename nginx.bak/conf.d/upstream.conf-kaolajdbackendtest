# -*- Mode: Nginx -*-
# KAOLA internal admin site for beta test
# *-kl.netease.com  CNAME portal-admin-kl.netease.com.

upstream kaola-carry-online-beta {
    upsync 127.0.0.1:8500/v1/kv/nginx/clusters/haitaobackendtest/product/upstreams/kaola-carry-online-beta upsync_timeout=6m upsync_interval=30s upsync_type=consul strong_dependency=off;
    upsync_dump_path /home/srv/nginx/kaola-carry-online-beta.conf;
    include /home/srv/nginx/kaola-carry-online-beta.conf;
}
