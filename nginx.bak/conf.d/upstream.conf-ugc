upstream ugcv.ws.netease.com {
    server 172.16.80.57:8080;
    server 172.16.80.58:8080;
    server 172.16.80.104:8080;
    server 172.16.80.103:8080;
    server 172.16.80.105:8080;
    server 172.16.80.90:8080;
    server 172.16.80.102:8080;
    server 172.16.80.89:8080;
    server 172.16.80.73:8080;
    server 10.170.164.40:8080;
    check interval=3000 rise=3 fall=3 timeout=5000 type=http port=8080;
    check_http_send "HEAD /health/status HTTP/1.0\r\n\r\n";
    check_http_expect_alive http_2xx http_3xx;
    keepalive 512;
}
